{
  "name": "truly-ui",
  "title":"Truly-UI",
  "description":"Web Components for Desktop Applications.",
  "version": "0.0.0-development",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n Truly-UI -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --force --project tsconfig.json ",
    "test": "tsc && karma start",
    "semantic-release": "semantic-release pre && npm publish --access public && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TemainfoSistemas/truly-ui.git"
  },
  "author": {
    "name": "Maicon Wagner dos Santos",
    "email": "mister-x59@hotmail.com.br"
  },
  "keywords": [
    "angular",
    "ui",
    "desktop",
    "ng",
    "electron",
    "angular2"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TemainfoSistemas/truly-ui.git/issues"
  },
  "dependencies": {
    "@angular/common": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.3"
  },
  "devDependencies": {
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.1.2",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.7",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.41.6",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "semantic-release": "^6.3.6",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
